<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY cc "http://creativecommons.org/ns#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY wikibase "http://wikiba.se/ontology#">
]>

<rdf:RDF
  xmlns:rdf="&rdf;"
  xmlns:rdfs="&rdfs;"
  xmlns:owl="&owl;"
  xmlns:cc="&cc;"
>

  <owl:Ontology rdf:about="http://wikiba.se/entityschema/ontology#">
    <rdfs:label>EntitySchema ontology</rdfs:label>
    <rdfs:comment>The system ontology of the EntitySchema extension</rdfs:comment>
    <rdfs:seeAlso rdf:resource="https://www.mediawiki.org/wiki/Extension:EntitySchema/RDF_mapping" />
    <owl:imports rdf:resource="http://wikiba.se/ontology#" />
    <cc:licence rdf:resource="http://creativecommons.org/publicdomain/zero/1.0/" />
  </owl:Ontology>

  <!--
  ///////////////////////////////////////////////////////////////////////////////////////
  //
  // Individuals
  //
  ///////////////////////////////////////////////////////////////////////////////////////
  -->

  <owl:NamedIndividual rdf:about="&wikibase;WikibaseEntitySchema">
    <rdfs:label>EntitySchema</rdfs:label>
    <rdfs:comment>Type for referring to an EntitySchema</rdfs:comment>
    <rdf:type rdf:resource="&wikibase;PropertyType"/>
  </owl:NamedIndividual>

</rdf:RDF>
